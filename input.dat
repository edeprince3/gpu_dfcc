sys.path.insert(0,'../.')

import gpu_dfcc

molecule {
O
H 1 1.0
H 1 1.0 2 104.5
}

memory 8000 mb
set {
  cc_timings true
  num_gpus 1
  df_basis_cc aug-cc-pvdz-ri
  df_basis_scf aug-cc-pvdz-jkfit
  basis        aug-cc-pvdz
  freeze_core true
  e_convergence 1e-8
  d_convergence 1e-8
  r_convergence 1e-8
  scf_type df
  maxiter 30
}

# run gpu df-ccsd
energy('gpu-df-ccsd')

clean()
